@using System.Configuration
@using MvcReportViewer;
@{
    ViewBag.Title = "MvcReportViewer Multiple Report Parameters Example";
}

@section Code {
    @@{
    var parameters = new List<keyvaluepair<string, object>>
        {
            new KeyValuePair<string, object>("Parameter1", "Value 1"),
            new KeyValuePair<string, object>("Parameter1", "Value 2"),
            new KeyValuePair<string, object>("Parameter2", DateTime.Now),
            new KeyValuePair<string, object>("Parameter2", DateTime.Now.AddYears(10)),
            new KeyValuePair<string, object>("Parameter3", 12345),
            new KeyValuePair<string, object>("Parameter4", null)
        };
    }

    @@Html.MvcReportViewerFluent(ConfigurationManager.AppSettings[WebConfigSettings.TestReportName]).ReportParameters(5parameters).Attributes(new { Height = 900, Width = 900, style = "border: none" })
}

