<!DOCTYPE html>
<html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <link href="~/Content/styles.css" rel="stylesheet" />
        <title>@ViewBag.Title</title>
        <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
    </head>
    <body>
        <nav>
            <ul>
                <li>@Html.ActionLink("Basic Interface", "Index", "Home")</li>
                <li>@Html.ActionLink("Fluent Interface", "Fluent", "Home")</li>
                <li>@Html.ActionLink("Post Report Parameters", "Post", "Home")</li>
                <li>@Html.ActionLink("Multiple Report Parameters", "Multiple", "Home")</li>
                <li>@Html.ActionLink("Multiple ReportParameter Parameters", "VisibilityCheck", "Home")</li>
                <li>@Html.ActionLink("Local Reports", "LocalReports", "Home")</li>
                <li>@Html.ActionLink("Local Reports (no data)", "NoDataLocalReport", "Home")</li>
                <li>@Html.ActionLink("Subreport", "Subreport", "Home")</li>
                <li>@Html.ActionLink("Drill-Through", "DrillThrough", "Home")</li>
                <li>@Html.ActionLink("Excel Report (Local)", "DownloadExcel", "Home")</li>
                <li>@Html.ActionLink("Word Report (Remote)", "DownloadWord", "Home")</li>
                <li>@Html.ActionLink("PDF Report  (Remote)", "DownloadPdf", "Home")</li>
                <li>@Html.ActionLink("PDF Report  (Local, no data)", "DownloadPdfLocalNoData", "Home")</li>
            </ul>
        </nav>
        <article>
            <section>
                <pre>
                    @RenderSection("Code", false)
                </pre>
            </section>
            <section>
                @RenderBody()
            </section>
        </article>
    </body>
</html>